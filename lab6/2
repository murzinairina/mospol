#include <iostream>
using namespace std;

int main()
{
	int A, B, C, d;
	setlocale(0, "");
	cout << "Введите число А: " << "\n";
	cin >> A;
	cout << "Введите число B: " << "\n";
	cin >> B;
	cout << "Введите число C: " << "\n";
	cin >> C;
	d = B;
	B = A;
	A = C;
	C = d;
	cout << "Поменяем местами значения А, B, C" << "\n";
	cout << "A: ";
	cout << A << "\n";
	cout << "B: ";
	cout << B << "\n";
	cout << "C: ";
	cout << C << "\n";
}
